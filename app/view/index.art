{{extend './layout.art'}}

{{block 'content'}}
<img src="images/egg-logo.png" class="logo">
<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<div class="avatar"></div>
<div id="app">
  <p>${ post.title }</p>
  <p>${ post.body }</p>
</div>
{{/block}}

{{block 'js'}}
<script src="{{@ helper.mix('/js/index.js')}}"></script>
{{/block}}
